[run]
source = .
omit = 
    */tests/*
    */venv/*
    */.venv/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*:\s*$
    @.*\s*$
    def .*:\s*$
    pass

show_missing = True
skip_covered = False
precision = 2

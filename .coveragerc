[run]
source = .
omit = 
    */tests/*
    */test_*
    */venv/*
    */.venv/*
    */__pycache__/*
    */setup.py
    */__main__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    @abc.abstractmethod
    @property

show_missing = True
skip_covered = False
precision = 2
fail_under = 80

[html]
directory = htmlcov
title = Test Coverage Report